<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="teamb.minicap.phaze">
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>

        <receiver android:name=".HeadsetPlugReceiver">
        <intent-filter>
            //maybe not necessary: (or it is necessary for registering of broadcast
            <action android:name="teamb.minicap.phaze.CUSTOM_INTENT"/>
            //action android:name="android.intent.action.HEADSET_PLUG"/>
            //headset plug is a potected broadcast, only the system can send it
        </intent-filter>
        </receiver>

        <service android:name=".Service_Music"/>
        <activity android:name=".MyoConnect" />
        <activity android:name=".Music"/>
        <activity android:name=".Video" />
        <activity android:name=".Gallery" />
        <activity android:name=".Settings"/>

        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
